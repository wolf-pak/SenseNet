
@{
    ViewBag.Title = "Index";
}

<h2>SensorData</h2>

@Html.Hidden("Get", Url.Action("Get", "Data"))

<body id="body" onload="startTime(), startDate()">

    <h1>DAGENS DATUM - <span id="date">CURRENT DATE</span></h1>
    <h2>
        <span id="txt">CURRENT TIME</span>
    </h2>

    <table class="table">

        <thred>
            <tr>
                <th>Id</th>
                <th>Time</th>
                <th>Value</th>
                <th>Type</th>
                <th>Node</th>
                <th>Sensor</th>
            </tr>

            <tbody id="tblInfo"></tbody>

        </thred>
    </table>

</body>

<script src="@Url.Content("~/Scripts/Javascript/time.js")" type="text/javascript"></script>
  
